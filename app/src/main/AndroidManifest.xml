<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.monkey.testskippage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SecondActivity">
            <intent-filter>
                <action android:name="testskippage.intent.action.skip"/>
                <action android:name="testskippage.intent.action.switch"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="testskippage.intent.category.skip"/>
                <category android:name="testskippage.intent.category.switch"/>
            </intent-filter>
        </activity>


        <activity android:name=".ThridActivity">
            <intent-filter>
                <action android:name="testskippage.intent.action.skip"/>
                <action android:name="testskippage.intent.action.switch"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>